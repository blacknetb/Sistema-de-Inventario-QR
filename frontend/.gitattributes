# *.png binary
# *.jpg binary
# *.jpeg binary
# *.gif binary
# *.ico binary
# *.svg binary
# *.woff binary
# *.woff2 binary
# *.ttf binary
# *.eot binary
# *.pdf binary
# *.zip binary
# *.tar.gz binary

# # Lock files
# package-lock.json -diff
# yarn.lock -diff
# pnpm-lock.yaml -diff

# # Environment files
# .env.example text eol=lf
# .env.development text eol=lf
# .env.production text eol=lf
# .env.test text eol=lf

